GENERATED_NAME="Generated"
DESTINATION_DIR="${SRCROOT}"
GEN_DIR="${DESTINATION_DIR}/${GENERATED_NAME}"

mkdir -p $GEN_DIR

echo $CONFIGURATION
echo "Destination: ${DESTINATION_DIR}"
echo "Gen: ${GEN_DIR}"
echo "Schema: ${SCHEMA_PATH}"
echo "Working dir: $(pwd)"

/tmp/GryphinGenerate -s "${SCHEMA_PATH}" -d "${GEN_DIR}"
